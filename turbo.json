{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "dev": {
      "cache": false,
      "dependsOn": ["build-src"]
    },
    "typecheck": {
      "dependsOn": ["^build"]
    },
    "lint": {},
    "clean:build": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["lib/**"]
    },
    "@gluestik/frontend#build": {
      "dependsOn": ["^build", "build:src"],
      "outputs": [".next/**"]
    },
    "build:src": {
      "outputs": ["lib/**"]
    },
    "export": {
      "dependsOn": ["^export", "build"],
      "outputs": ["out/**"]
    },
    "test": {
      "dependsOn": ["^build"]
    },
    "deploy": {
      "dependsOn": ["^export", "^build"],
      "cache": false
    },
    "synth": {
      "dependsOn": ["^export"]
    }
  }
}
