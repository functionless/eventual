{
  "name": "@eventual/integrations-slack",
  "version": "0.57.0",
  "type": "module",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "@slack/bolt": "^3.12.2",
    "@slack/logger": "^3.0.0",
    "@slack/web-api": "^6.8.1",
    "tsscmp": "^1.0.6"
  },
  "peerDependencies": {
    "@eventual/core": "workspace:^",
    "itty-router": "^2.6.6"
  },
  "devDependencies": {
    "@eventual/core": "workspace:^",
    "@types/tsscmp": "^1.0.0",
    "itty-router": "2.6.6",
    "jest": "^29",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ulidx": "^0.3.0"
  },
  "jest": {
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.test.json",
          "useESM": true
        }
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
