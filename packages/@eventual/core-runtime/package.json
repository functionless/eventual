{
  "name": "@eventual/core-runtime",
  "version": "0.36.0",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "dependencies": {
    "@eventual/core": "workspace:^",
    "heap-js": "2.2.0",
    "itty-router": "^2.6.6",
    "ulidx": "^0.3.0",
    "zod": "^3"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@types/jest": "^29",
    "@types/node": "^18",
    "jest": "^29",
    "ts-jest": "^29",
    "ts-node": "^10.9.1",
    "typescript": "^5"
  },
  "jest": {
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.test.json",
          "useESM": true
        }
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
