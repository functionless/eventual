{
  "name": "@eventual/core-runtime",
  "version": "0.57.0",
  "exports": {
    ".": {
      "import": "./lib/index.js"
    }
  },
  "type": "module",
  "module": "./lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "dependencies": {
    "@eventual/core": "workspace:^",
    "@opensearch-project/opensearch": "^2.2.1",
    "heap-js": "2.2.0",
    "itty-router": "^2.6.6",
    "ulidx": "^0.3.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "jest": "^29",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  },
  "jest": {
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "transformIgnorePatterns": [
      "node_modules/",
      "node_modules/@eventual/core"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.test.json",
          "useESM": true
        }
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
