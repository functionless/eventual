{
  "name": "@eventual/compiler",
  "version": "0.57.0",
  "bin": {
    "eventual-bundle": "./bin/eventual-bundle.mjs",
    "eventual-infer": "./bin/eventual-infer.mjs"
  },
  "exports": {
    ".": {
      "import": "./lib/index.js"
    },
    "./bin/eventual-bundle.mjs": {
      "require": "./bin/eventual-bundle.mjs"
    },
    "./bin/eventual-infer.mjs": {
      "require": "./bin/eventual-infer.mjs"
    }
  },
  "module": "./lib/index.js",
  "types:": "lib//index.d.ts",
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --passWithNoTests"
  },
  "peerDependencies": {
    "esbuild": ">=0.16.x <1.0.0"
  },
  "dependencies": {
    "@anatine/zod-openapi": "^1.12.0",
    "@eventual/core": "workspace:^",
    "@swc/core": "^1.2.245",
    "esbuild-plugin-alias-path": "^2.0.2",
    "openapi3-ts": "^3.1.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/minimatch": "5.1.2",
    "esbuild": "^0.17.4",
    "jest": "^29",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/",
      "/packages/@eventual/core/"
    ],
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.test.json",
          "useESM": true
        }
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
