{
  "name": "@eventual/cli",
  "type": "module",
  "version": "0.33.3",
  "bin": {
    "eventual": "bin/eventual.mjs"
  },
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --passWithNoTests"
  },
  "devDependencies": {
    "@swc/core": "^1.3.19",
    "@swc/jest": "^0.2.23",
    "@types/express": "^4.17.14",
    "@types/jest": "^29",
    "@types/node": "^18",
    "@types/serve-static": "^1.15.0",
    "@types/yargs": "^17.0.13",
    "jest": "^29",
    "ts-jest": "^29",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "ts-jest",
        {
          "useESM": true,
          "tsconfig": "tsconfig.test.json"
        }
      ]
    },
    "transformIgnorePatterns": [
      ".*/.pnpm/.*"
    ],
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "#ansi-styles": "ansi-styles/index.js",
      "#supports-color": "supports-color/index.js",
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.254.0",
    "@aws-sdk/client-ssm": "^3.254.0",
    "@aws-sdk/client-sts": "^3.254.0",
    "@aws-sdk/config-resolver": "^3.254.0",
    "@aws-sdk/node-config-provider": "^3.254.0",
    "@aws-sdk/types": "^3.254.0",
    "@eventual/aws-client": "workspace:^",
    "@eventual/client": "workspace:^",
    "@eventual/compiler": "workspace:^",
    "@eventual/core": "workspace:^",
    "@eventual/core-runtime": "workspace:^",
    "@eventual/project": "workspace:^",
    "@eventual/timeline": "workspace:^",
    "chalk": "^5.1.2",
    "cli-table3": "^0.6.3",
    "express": "^4.18.2",
    "get-port": "^6.1.2",
    "get-stdin": "^9.0.0",
    "import-meta-resolve": "^2.2.0",
    "open": "^8.4.0",
    "ora": "^6.1.2",
    "serve-static": "^1.15.0",
    "ts-node": "^10.9.1",
    "vite": "^3.2.3",
    "yargs": "^17.6.2"
  }
}
