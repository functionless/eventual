{
  "name": "@eventual/core",
  "version": "0.42.0",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    },
    "./internal": {
      "import": "./lib/esm/internal/index.js",
      "require": "./lib/cjs/internal/index.js"
    }
  },
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "files": [
    "internal",
    "lib"
  ],
  "scripts": {
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.7.0",
    "@opensearch-project/opensearch": "^2.2.1",
    "itty-router": "^2.6.6",
    "openapi3-ts": "^3.1.2",
    "ulidx": "^0.3.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@tshttp/status": "^2.0.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^18",
    "jest": "^29",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "type-fest": "^3.11.0",
    "typescript": "^5",
    "ulidx": "^0.3.0"
  },
  "jest": {
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.test.json",
          "useESM": true
        }
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
