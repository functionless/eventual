{
  "name": "@eventual/client",
  "version": "0.57.0",
  "type": "module",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --passWithNoTests"
  },
  "dependencies": {
    "@eventual/core": "workspace:^"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "esbuild": "0.19.11",
    "ts-node": "^10.9.1"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.test.json",
          "useESM": true
        }
      ]
    },
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
