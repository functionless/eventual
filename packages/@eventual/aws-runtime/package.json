{
  "name": "@eventual/aws-runtime",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "version": "0.0.0",
  "scripts": {
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "@eventual/core": "workspace:^",
    "aws-lambda": "^1.0.7",
    "fast-equals": "^4.0.3",
    "micro-memoize": "^4.0.11",
    "ulid": "^2.3.0",
    "aws-embedded-metrics": "^4.0.0"
  },
  "peerDependencies": {
    "@aws-sdk/client-dynamodb": "^3.208.0",
    "@aws-sdk/client-lambda": "^3.208.0",
    "@aws-sdk/client-s3": "^3.208.0",
    "@aws-sdk/client-sqs": "^3.208.0",
    "@types/aws-lambda": "8.10.108"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "3.208.0",
    "@aws-sdk/client-lambda": "^3.208.0",
    "@aws-sdk/client-s3": "3.208.0",
    "@aws-sdk/client-sqs": "3.208.0",
    "@types/aws-lambda": "8.10.108",
    "@types/jest": "^29",
    "@types/node": "^16",
    "jest": "^29",
    "ts-jest": "^29",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "jest": {
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.test.json",
          "useESM": true
        }
      ]
    },
    "moduleNameMapper": {
      "@eventual/injected/(.*)": "<rootDir>/src/injected/$1",
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
