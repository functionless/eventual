{
  "name": "@eventual/aws-runtime",
  "exports": {
    ".": {
      "default": "./lib/esm/index.js"
    }
  },
  "module": "./lib/esm/index.js",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^1.4.1",
    "@eventual/aws-env": "workspace:^",
    "@eventual/core": "workspace:^",
    "@middy/core": "^3.6.2",
    "@middy/error-logger": "^3.6.2",
    "aws-embedded-metrics": "^4.0.0",
    "aws-lambda": "^1.0.7",
    "node-fetch": "^3.3.0",
    "ulidx": "^0.3.0"
  },
  "peerDependencies": {
    "@aws-sdk/client-dynamodb": "^3.208.0",
    "@aws-sdk/client-lambda": "^3.208.0",
    "@aws-sdk/client-s3": "^3.208.0",
    "@aws-sdk/client-scheduler": "^3.208.0",
    "@aws-sdk/client-sqs": "^3.208.0",
    "@types/aws-lambda": "8.10.108",
    "itty-router": "^2.6.6"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "3.214.0",
    "@aws-sdk/client-lambda": "^3.213.0",
    "@aws-sdk/client-s3": "3.213.0",
    "@aws-sdk/client-scheduler": "3.213.0",
    "@aws-sdk/client-sqs": "3.213.0",
    "@types/aws-lambda": "8.10.108",
    "@types/jest": "^29",
    "@types/node": "^16",
    "itty-router": "2.6.6",
    "jest": "^29",
    "ts-jest": "^29",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "jest": {
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.test.json",
          "useESM": true
        }
      ]
    },
    "moduleNameMapper": {
      "@eventual/injected/(.*)": "<rootDir>/src/injected/$1",
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
